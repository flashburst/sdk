{
  "version": "0.5.4-beta",
  "name": "@neptunemutual/sdk",
  "author": "Neptune Mutual Team",
  "description": "Neptune Mutual SDK",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c --environment NODE_ENV:production",
    "compile": "yarn build",
    "dev": "rollup --watch -c",
    "standard": "ts-standard",
    "fix": "ts-standard --fix",
    "publish": "npm run build && npm publish"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "5.4.1",
    "@ethersproject/constants": "5.4.0",
    "@ethersproject/contracts": "5.4.1",
    "@ethersproject/providers": "5.4.5",
    "@ethersproject/solidity": "5.4.0",
    "@ethersproject/strings": "5.4.0",
    "axios": "0.26.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "5.0.2",
    "@types/node": "18.11.11",
    "@types/uuid": "8.3.4",
    "rollup": "3.5.1",
    "rollup-plugin-typescript2": "0.34.1",
    "ts-standard": "11.0.0",
    "typescript": "4.9.3"
  },
  "ts-standard": {
    "ignore": [
      "src/packages/**/*.ts"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neptune-mutual-blue/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/neptune-mutual-blue/sdk/issues"
  },
  "homepage": "https://github.com/neptune-mutual-blue/sdk#readme",
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.19"
  }
}
